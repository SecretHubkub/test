_G.Autoskill = true
while _G.Autoskill do wait()
    local args = {
        [1] = {
            ["Goal"] = "Console Move",
            ["Tool"] = game:GetService("Players").LocalPlayer:WaitForChild("Normal Punch")
        }
    }
    
    game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
    wait(.5)
    local args = {
        [1] = {
            ["Goal"] = "Console Move",
            ["Tool"] = game:GetService("Players").LocalPlayer:WaitForChild("Consecutive Punches")
        }
    }
    
    game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
    wait(.5)
    local args = {
        [1] = {
            ["Goal"] = "Console Move",
            ["Tool"] = game:GetService("Players").LocalPlayer:WaitForChild("Shove")
        }
    }
    
    game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
    wait(.5)
    local args = {
        [1] = {
            ["Goal"] = "Console Move",
            ["Tool"] = game:GetService("Players").LocalPlayer:WaitForChild("Uppercut")
        }
    }
    
    game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
    wait(.5)     
end    
