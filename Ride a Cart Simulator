_G.Autofarm = true
while _G.Autofarm do wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1.83587829e-07, 389.049988, -88.4000015, -1, 0, 0, 0, 1, 0, 0, 0, -1)
    repeat game:GetService("Workspace").Carts.LocalPlayer.Archivable = false
    until game:GetService("Workspace").Carts.LocalPlayer.Archivable = true
end
