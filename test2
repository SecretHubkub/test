ฃ
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Secret Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "Ride a Cart Simulator"})

-- zone1


local Tab = Window:MakeTab({
	Name = "Zone1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Auto Farm"
})
Tab:AddToggle({
	Name = "Auto collect!",
	Default = false,
	Callback = function(Value)
        _G.Autocollect_zone1 = Value
        while _G.Autocollect_zone1 do wait()
            pcall(function()
                for i, v in pairs(game:GetService("Workspace").Effects:GetChildren()) do
                    if v.Name == "Template" then
                        v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                        wait(.1)
                    end
                end  -- รอเพิ่มเวลาที่เหมาะสมที่นี่
            end)
        end
	end    
})
Tab:AddToggle({
	Name = "Auto Farm!",
	Default = false,
	Callback = function(Value)
        _G.Autofarm = Value --ออโต้ฟาม
        while _G.Autofarm do wait()
            pcall(function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-16000, 389.049988, -88.4000015, -1, 0, 0, 0, 1, 0, 0, 0, -1)
                wait(2)
            end)
        end    
    end    
})

local Section = Tab:AddSection({
	Name = "Auto Egg"
})


Tab:AddToggle({
	Name = "Auto Egg!",
	Default = false,
	Callback = function(Value)
        _G.Autoegg = Value --auto egg1 zone1
        while _G.Autoegg do wait()
            pcall(function()
                local args = {
                    [1] = "Egg",
                    [2] = 1
                }
        
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Shop"):WaitForChild("ClientBuyEgg"):InvokeServer(unpack(args))
                wait(6)
            end)
        end
	end    
})

Tab:AddToggle({
	Name = "Auto Rocky Egg",
	Default = false,
	Callback = function(Value)
        _G.Rocky_egg = Value --auto egg2 zone1
        while _G.Rocky_egg do wait()
            pcall(function()
        
                local args = {
                    [1] = "Rocky Egg",
                    [2] = 1
                }
            
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Shop"):WaitForChild("ClientBuyEgg"):InvokeServer(unpack(args))
                wait(6)
            end)       
        end    
	end    
})

