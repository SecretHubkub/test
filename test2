--teleport
local Tab = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "teleport"
})

Tab:AddButton({
	Name = "Zone1",
	Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-16000, 389.600006, -142, -1, 0, 0, 0, 1, 0, 0, 0, -1) -- teleport to zone1
  	end    
})
Tab:AddButton({
	Name = "Zone2",
	Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(40.0824432, 396.127197, -150.096176, 0.0868716463, -9.17021268e-08, -0.996219516, 2.43471937e-10, 1, -9.20288841e-08, 0.996219516, 7.75214914e-09, 0.0868716463) -- teleport to zone2
  	end    
})
Tab:AddButton({
	Name = "Zone3",
	Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(16031.9854, 393.331573, -133.927292, -0.0494210906, -3.02798355e-08, -0.998778045, 4.01200739e-10, 1, -3.03367322e-08, 0.998778045, -1.89998484e-09, -0.0494210906) --teleport to zone3
  	end    
})
