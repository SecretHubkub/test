  	end    
})
Tab:AddButton({
	Name = "Zone5",
	Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(48001.1172, 389.600281, -146.599518, -1, 0, 0, 0, 1, 0, 0, 0, -1) --teleport to zone5
