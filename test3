    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
    local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
    local Tab = Window:MakeTab({
        Name = "Auto Farm",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    local Section = Tab:AddSection({
        Name = "AutoFarm"
    })

    Tab:AddToggle({
        Name = "Auto Equip",
        Default = false,
        Callback = function(Value)
            _G.Auto_Equip = Value
            while _G.Auto_Equip do wait()
                pcall(function()
                    local weapon = "None"
                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").fameshop0828.Backpack:FindFirstChild(weapon))
                end)
            end
        end    
    })
    Tab:AddToggle({
        Name = "Auto hit",
        Default = false,
        Callback = function(Value)
            _G.Autohit = Value
            while _G.Autohit do wait()
                game:GetService'VirtualUser':CaptureController()
                game:GetService'VirtualUser':Button1Down(Vector2.new(474,204))
            end
        end    
    })
    Tab:AddToggle({
        Name = "Auto Farm boss",
        Default = false,
        Callback = function(Value)
            _G.AutoFarm_boss = Value
            while _G.AutoFarm_boss do wait()
                pcall(function()
                    local warp = game.Players.LocalPlayer.Character.HumanoidRootPart
                    for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetDescendants()) do
                        if v.ClassName == "Model" then
                            warp.CFrame = v.HumanoidRootPart.CFrame*CFrame.new(0,0,3)
                    
                        end
                    end
                end)
            end
        end    
    })

    OrionLib:Init()
