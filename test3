-- 5/5/2024

function autofarm()
    pcall(function()
        local rebirth = game:GetService("Players").LocalPlayer.leaderstats.Rebirths.Value
        local warp = game.Players.LocalPlayer.Character.HumanoidRootPart
        if rebirth <= 0 then
            warp.CFrame = CFrame.new(-16000, 389.049988, -88.4000015, -1, 0, 0, 0, 1, 0, 0, 0, -1)
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Data"):WaitForChild("RequestRebirth"):InvokeServer()
        elseif rebirth >= 1 then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(64000, 385.550293, -88.3991394, -1, 0, 0, 0, 1, 0, 0, 0, -1)
            task.wait(5)
        end
    end)
end

function autocollect()
    pcall(function()
        for i, v in pairs(game:GetService("Workspace").Effects:GetChildren()) do
            if v.Name == "Template" then
                v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            end
        end
    end)
end
local HttpRequest = http_request or request or syn.request
function sendWebhook()
    Webhook_URL = _G.Webhook_URL

    local response = HttpRequest({
        Url = Webhook_URL,
        Method = "POST",
        Headers = {
            ['Content-Type'] = 'application/json'
        },
        Body = HttpService:JSONEncode({
            ["content"] = "",
            ["embeds"] = {{
                ["title"] = "Ride a Cart SimulatorüòÅ",
                ["description"] = "USERNAME : " .. game.Players.LocalPlayer.DisplayName,
                ["type"] = "rich", -- ‡∏õ‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡πá‡∏ô "rich" ‡∏´‡∏£‡∏∑‡∏≠ "embed" ‡∏ï‡∏≤‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°
                ["color"] = tonumber(0xffffff),
                ["fields"] = {{
                    ["name"] = "YOUR GEM",
                    ["value"] = game:GetService("Players").LocalPlayer.leaderstats.Gems.Value, -- ‡πÄ‡∏û‡∏¥‡πà‡∏° .Value ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏Ç‡πâ‡∏≤‡∏ñ‡∏∂‡∏á‡∏Ñ‡πà‡∏≤
                    ["inline"] = true
                }}
            }}
        })
    })
end

local HttpRequest = http_request or request or syn.request
-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏Å‡πá‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏Å‡∏≤‡∏£‡∏™‡πà‡∏á webhook
local webhookSent = false

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏™‡πà‡∏á webhook
function sendWebhook2()
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤ webhook ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡∏™‡πà‡∏á
    if not webhookSent then
        HttpService = game:GetService("HttpService")
        Webhook_URL = "https://discord.com/api/webhooks/1232594929109434389/HIRL7cCe2d7u1ZnbeilJm74XmDl2chnJxVGFZZSvxeT7etxzf1IpEP2RNYz4QD2R2ehk"

        -- ‡∏™‡πà‡∏á webhook
        local response = HttpRequest({
            Url = Webhook_URL,
            Method = "POST",
            Headers = {
                ['Content-Type'] = 'application/json'
            },
            Body = HttpService:JSONEncode({
                ["content"] = "",
                ["embeds"] = {{
                    ["title"] = "Ride a Cart Simulator (AF)üòÅ",
                    ["description"] = "USERNAME : " .. game.Players.LocalPlayer.DisplayName,
                    ["type"] = "rich", -- ‡∏õ‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡πá‡∏ô "rich" ‡∏´‡∏£‡∏∑‡∏≠ "embed" ‡∏ï‡∏≤‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°
                    ["color"] = tonumber(0xffffff),
                    ["fields"] = {{
                        ["name"] = "HWID",
                        ["value"] = game:GetService("RbxAnalyticsService"):GetClientId(), -- ‡πÄ‡∏û‡∏¥‡πà‡∏° .Value ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏Ç‡πâ‡∏≤‡∏ñ‡∏∂‡∏á‡∏Ñ‡πà‡∏≤
                        ["inline"] = true
                    }}
                }}
            })
        })

        -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞ webhook ‡πÉ‡∏´‡πâ‡πÄ‡∏õ‡πá‡∏ô true ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏™‡πà‡∏á‡πÅ‡∏•‡πâ‡∏ß
        webhookSent = true
    end
end


function GUI ()
    -- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Gems = Instance.new("TextLabel")
local Name = Instance.new("TextLabel")

--Properties:

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.BackgroundTransparency = 0.400
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(-0.382021487, 0, -0.0272952858, 0)
Frame.Size = UDim2.new(0, 88305, 0, 53306)

Gems.Name = "Gems"
Gems.Parent = ScreenGui
Gems.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Gems.BackgroundTransparency = 1.000
Gems.BorderColor3 = Color3.fromRGB(0, 0, 0)
Gems.BorderSizePixel = 0
Gems.Position = UDim2.new(0.325851381, 0, 0.468982637, 0)
Gems.Size = UDim2.new(0, 200, 0, 50)
Gems.Font = Enum.Font.Unknown
Gems.Text = "üíéGemsüíé : " .. 
Gems.TextColor3 = Color3.fromRGB(0, 0, 0)
Gems.TextSize = 30.000

Name.Name = "Name"
Name.Parent = ScreenGui
Name.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Name.BackgroundTransparency = 1.000
Name.BorderColor3 = Color3.fromRGB(0, 0, 0)
Name.BorderSizePixel = 0
Name.Position = UDim2.new(0.325851381, 0, 0.297766745, 0)
Name.Size = UDim2.new(0, 200, 0, 50)
Name.Font = Enum.Font.Unknown
Name.Text = "Name : " .. game.Players.LocalPlayer.DisplayName
Name.TextColor3 = Color3.fromRGB(0, 0, 0)
Name.TextSize = 30.000
end


function upGem ()
    _G.k = true
    while _G.k do wait()
local updateGem = game.Players.LocalPlayer.leaderstats.Gems.Value
task.wait(10)
end
_G.Auto = true
spawn(function()
    while task.wait() do
        if _G.Auto then
            autofarm()
        end
    end
end)
_G.ka = true
task.spawn(function()
    while task.wait() do
        if _G.ka then
            autocollect()
        end
    end
end)

spawn(function() -- webhook
    while true do
        sendWebhook()
        wait(600)
    end
end)


spawn(function()
    sendWebhook2()
end)



spawn(function()
    GUI()
end)


spawn(function()
    upGem()
end)
